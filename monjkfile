pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo "hello à tous" 
                }
            }
         stage('verification') {
            steps {
                echo "etape de vérification"
                }
             }
          stage('deploiment') {
            steps {
                echo "etape d'emploiement" 
                  }
             }
         }
}
